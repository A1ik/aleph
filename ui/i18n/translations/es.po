#. [searchTips.composite.2]
#. defaultMessage is:
#. <em>+banana ice cream</em> requires that only results with the term banana
#. will be returned, while ice and cream are optional
#: i18n/messages.json
msgid ""
"<em>+banana ice cream</em> requires that only results with the term banana "
"will be returned, while ice and cream are optional"
msgstr ""
"<em>+helado de banana</em> significa que solo se devolverán resultados con "
"el término helado, mientras que \"de\" y \"banana\" serán opcionales"

#. [searchTips.composite.3]
#. defaultMessage is:
#. <em>-banana ice cream</em> ensures that no results with the term banana
#. will be returned
#: i18n/messages.json
msgid ""
"<em>-banana ice cream</em> ensures that no results with the term banana will"
" be returned"
msgstr ""
"<em>-helado de banana</em> garantiza que no se devolverán resultados con el "
"término helado"

#. [searchTips.composite.4]
#. defaultMessage is:
#. <em>banana AND “ice cream”</em> requires that only results with both banana
#. and ice cream are returned
#: i18n/messages.json
msgid ""
"<em>banana AND “ice cream”</em> requires that only results with both banana "
"and ice cream are returned"
msgstr ""
"<em>helado AND “de banana”</em> indica que se devolverán solo resultados con"
" los términos \"helado\" y \"de banana\""

#. [searchTips.composite.5]
#. defaultMessage is:
#. <em>banana OR “ice cream”</em> ensures that only results with either banana
#. or ice cream are returned
#: i18n/messages.json
msgid ""
"<em>banana OR “ice cream”</em> ensures that only results with either banana "
"or ice cream are returned"
msgstr ""
"<em>helado OR “de banana”</em> indica que se devolverán solo resultados con "
"los términos \"helado\" o \"de banana\""

